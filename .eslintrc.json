{
  "env":{
    "es6": true,
    "jest/globals": true
  },
  "plugins": [
    "import",
    "jest",
    "prefer-object-spread",
    "promise",
    "security",
    "standard"
  ],
  "rules": {
    "camelcase": [0],
    "no-shadow": [1],
    "max-len": [1],
    "no-underscore-dangle": [0],
    "arrow-parens": ["error", "as-needed"]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
